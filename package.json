{
  "name": "fin-pro-types",
  "version": "1.0.6",
  "description": "Shared TypeScript types, interfaces, and DTOs for loan management system",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "CHANGELOG.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "extract-types": "node scripts/extract-types.js",
    "validate-consistency": "node scripts/validate-consistency.js",
    "generate-docs": "node scripts/generate-docs.js",
    "prepublishOnly": "npm run build && npm run test",
    "pre-publish": "node scripts/pre-publish.js",
    "publish-package": "node scripts/publish.js",
    "release": "node scripts/release.js",
    "clean": "rimraf dist",
    "dev": "npm run build:watch"
  },
  "keywords": [
    "typescript",
    "types",
    "dto",
    "interfaces",
    "enums",
    "loan-management",
    "shared-types"
  ],
  "author": "Your Organization",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/kmrschnio/types.git"
  },
  "bugs": {
    "url": "https://github.com/kmrschnio/types/issues"
  },
  "homepage": "https://github.com/kmrschnio/types#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-typescript": "^11.0.0",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.57.1",
    "jest": "^29.5.0",
    "rimraf": "^5.0.0",
    "rollup": "^3.0.0",
    "rollup-plugin-dts": "^5.0.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "typescript": ">=4.9.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "tslib": "^2.8.1"
  }
}
